<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title><%- title %></title>
    <link><%- url %></link>
    <description><%- description %></description>
    <atom:link href="<%- url %>/rss.xml" rel="self" type="application/rss+xml" />
    <% for(name in public.blog.post.neuroscience._data) { %>
    <% var post = public.blog.post.neuroscience._data[name] %>
      <item>
        <title><%- post.title %></title>
        <description><![CDATA[<%- partial("/blog/post/neuroscience/" + name) %>]]></description>
        <pubDate><%- new Date(post.date) %></pubDate>
        <link><%- url %>/blog/post/neuroscience/<%- name %></link>
        <guid isPermaLink="true"><%- url %>/blog/post/neuroscience/<%- name %></guid>
      </item>
    <% } %>
  </channel>
</rss>
