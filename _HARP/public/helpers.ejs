<%
  parent = function () {
    var upper = current.path.length - 1;
    var parent = [];
    for (i = 0; i < upper; i++) {
      if (i !== upper - 1) {
        parent[i] = current.path[i];
      } else {
        parent[i] = current.path[i].slice(0,-4)
      }
    }
    return parent.join('/')
  }
%>
